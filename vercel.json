{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "backend/config/**",
          "backend/controllers/**",
          "backend/models/**",
          "backend/routes/**",
          "backend/cache/**",
          "backend/utils/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/index.ts"
    }
  ]
}
